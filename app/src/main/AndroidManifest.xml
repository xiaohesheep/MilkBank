<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="bafy.pharmacy.milkbank">

    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />
    <application
        android:allowBackup="true"
        android:icon="@mipmap/milkbank"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/milkbank"
        android:supportsRtl="true"
        android:theme="@style/AppTheme.NoActionBar">
        <activity android:name=".activity.HomeActivity"></activity>
        <activity android:name=".activity.LogonActivity" />
        <activity android:name=".activity.MainActivity" />
        <activity android:name=".activity.PxActivity" />
        <activity android:name=".ui.activity.OrderListActivity"></activity>
        <activity android:name=".activity.StartPageActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>